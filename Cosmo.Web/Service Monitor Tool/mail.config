	<!-- 004 -->
	<MailTemplateField>
		<Id>SubscriptionReport</Id>
		<IsHtml>true</IsHtml>
		<From>{{AdminAddress}}</From>
		<To>{{DistributionEmail}}</To>
		<Cc>{{CCAddress}}</Cc>
		<Bcc></Bcc>
		<ReplyTo>{{AdminAddress}}</ReplyTo>
		<Subject>Cosmo - Cognos BI Environment {{Type}} Service Status Report </Subject>
		<Attachment></Attachment>
		<Body>
			<![CDATA[<div style="font-family:Verdana">
			<p> Hi,<br><br>
              This message is to notify about Cognos BI Environment {{Type}} Service Status Report from {{StartDateTime}} to {{EndDateTime}}.
