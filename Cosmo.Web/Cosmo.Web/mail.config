<?xml version="1.0" encoding="utf-8" ?>
<ArrayOfMailTemplateField xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<!-- 001 -->
	<MailTemplateField>
		<Id>CQA Meeting Minutes</Id>
		<IsHtml>true</IsHtml>
		<From>{{AdminAddress}}</From>
		<To>{{LoggedInUser.EmailID}};{{ApproverEMailList}};{{DistributionGroupUsersEmail}}</To>
		<Cc></Cc>
		<Bcc></Bcc>
		<Subject>CQA Meeting Minutes</Subject>
		<Attachment>{{MeetingAttachments}}</Attachment>
		<Body>
			<![CDATA[<p> Hi,<br><br>
              This message is to notify you that CQA meeting is available for the below Projects:
              <br><br>
              <b>Project Details</b><br>
              {{ProjectDetails}}
              <br><br>
              
              Note: This email has been automatically generated by the CQA-SAM application. Please do not reply to this email.
             ]]>
		</Body>
	</MailTemplateField>
	<!-- 002 -->
	<MailTemplateField>
		<Id>Acknowledge</Id>
		<IsHtml>true</IsHtml>
		<From>{{AdminAddress}}</From>
		<To>{{DistributionEmail}}</To>
		<Cc>{{CCAddress}}</Cc>
		<Bcc></Bcc>
		<ReplyTo>{{AdminAddress}}</ReplyTo>
		<Subject>{{Env_Name}} Environment - {{ContentType}} Service Failure Acknowledgment Notice (Server/port – {{ServiceName}})</Subject>
		<Attachment></Attachment>
		<Body>
			<![CDATA[<div style="font-family:Verdana">
			<p> Hi,<br><br>
              This message is to notify that the below service failure has been acknowledged. 
              <br><br>
              <b>Notification Details</b><br>
              <hr>
			  <table style='width:1000px;border:0px solid #ccc;'>
				  <tr> 
					<td style='width:25%;padding:5px;'>Environment</td><td>: {{Env_Name}}</td>
				  </tr>
				  <tr>
					<td style='padding:5px;'>Service Type</td><td>: {{ContentType}}</td>
				  </tr>
				  <tr>
					<td style='padding:5px;'>Server / Port:</td><td>: {{ServiceName}}</td>
				  </tr>
				  <tr>
					<td style='padding:5px;'>Service Status</td><td>: {{Status}}</td>
				  </tr>
				  <tr>
					<td style='padding:5px;'>Acknowledged By:</td><td>: {{CreatedBy}}</td>
				  </tr>
				  <tr>
					<td style='padding:5px;'>Comments</td><td>: {{AcknowledgeComments}}</td>
				  </tr>
			  </table>
			  
              <br><br>
              
              Note: This email has been automatically generated by the Cosmo Application. Please do not reply to this email.
			  
			  </div>
			  <br><br><br>
              <div style="font-family:verdana;font-size:.85em"><i>This email has been automatically generated by Cosmo Application. Please do not reply to this email.</i></div>
             ]]>
		</Body>
	</MailTemplateField>

