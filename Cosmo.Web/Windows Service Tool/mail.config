<?xml version="1.0" encoding="utf-8" ?>
<ArrayOfMailTemplateField xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <!-- 001 -->
  <MailTemplateField>
    <Id>CQA Meeting Minutes</Id>
    <IsHtml>true</IsHtml>
    <From>{{AdminAddress}}</From>
    <To>{{LoggedInUser.EmailID}};{{ApproverEMailList}};{{DistributionGroupUsersEmail}}</To>
    <Cc></Cc>
    <Bcc></Bcc>
    <Subject>CQA Meeting Minutes</Subject>
    <Attachment>{{MeetingAttachments}}</Attachment>
    <Body>
      <![CDATA[<p> Hi,<br><br>
              This message is to notify you that CQA meeting is available for the below Projects:
              <br><br>
              <b>Project Details</b><br>
              {{ProjectDetails}}
              <br><br>
              
              Note: This email has been automatically generated by the CQA-SAM application. Please do not reply to this email.
             ]]>
    </Body>
  </MailTemplateField>

	<!-- 002 -->
	<MailTemplateField>
		<Id>ServiceOperationFailure</Id>
		<IsHtml>true</IsHtml>
		<From>{{AdminAddress}}</From>
		<To>{{DistributionEmail}}</To>
		<Cc>{{CCAddress}}</Cc>
		<Bcc></Bcc>
		<ReplyTo>{{AdminAddress}}</ReplyTo>
		<Subject>{{Env_Name}} Environment Services Schedule - {{Group_Schedule_Action}} Completion Status Notice – Unsuccessful for the service {{WindowsServiceName}}</Subject>
		<Attachment></Attachment>
		<Body>
			<![CDATA[<div style="font-family:verdana">
			<p> Hi,<br><br>
			  
			  This message is to notify that the below service is {{Group_ActionCompletion_Status}}.
			  <br><br><br>
              
              <b>Service Details</b><br><hr>
              Environment     :   {{Env_Name}} <br>
			  Service Type    :   {{Service_Name}} <br>
              Service Host    :   {{HostIP}} <br>
              Service Name    :   {{WindowsServiceName}} <br>
			  Last Updated Time: {{Schedule_UpdatedTime}} <br>


              <br>
			  Note: Monitor flag has been turned off for 'Timed Out or Unsuccessful' status, turned on manually, if required.
		  
			  </div>
			  <br><br><br>
              <div style="font-family:verdana;font-size:.85em"><i>This email has been automatically generated by Cosmo Application. Please do not reply to this email.</i></div>
			 ]]>
		</Body>
	</MailTemplateField>

	<MailTemplateField>
		<Id>ServiceOperationFailure_sms</Id>
		<IsHtml>false</IsHtml>
		<From>{{AdminAddress}}</From>
		<To>{{DistributionEmail}}</To>
		<ReplyTo>{{AdminAddress}}</ReplyTo>
		<Subject>Service Completion Status – Unsuccessful </Subject>
		<Attachment></Attachment>
		<Body>
			<![CDATA[{{Group_Name}}~{{Env_Name}}/{{Group_ActionCompletion_Status}}/by {{Group_Schedule_CreatedBy}}]]>
		</Body>
	</MailTemplateField>

	
